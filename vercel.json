{
    "version": 2,
    "builds": [
      {
        "src": "lembarpena/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9" 
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/admin/(.*)",
        "dest": "/static/admin/$1"
      },
      {
        "src": "/static/Gambar/(.*)",
        "dest": "/static/Gambar/$1"
      },
      {
        "src": "/static/Poppins/(.*)",
        "dest": "/static/Poppins/$1"
      },
      {
        "src": "/static/Script/(.*)",
        "dest": "/static/Script/$1"
      },
      {
        "src": "/static/Style/(.*)",
        "dest": "/static/Style/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "lembarpena/wsgi.py"
      }
    ]
}